stage('test-examples') {

    if (env.SGX == "1") {
        env.MODE = "sgx"
    }
    else {
        env.MODE = "direct"
    }

    if (["ubuntu", "debian"].contains(env.os_release_id)) {
           try {
                  sh '''
                        cd CI-Examples/mysql
                        sudo -E make install-dependencies
                        make all
                        make ${MAKEOPTS} check 2>&1 | tee RESULT
                    '''
            } catch (Exception e) { }
     }    
}
